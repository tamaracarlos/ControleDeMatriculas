<%- model_class = Pagamento -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:dinheiro) %>:</strong></dt>
  <dd><%= @pagamento.dinheiro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:troco) %>:</strong></dt>
  <dd><%= @pagamento.troco %></dd>
  
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              matriculas_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_pagamento_path(@pagamento), :class => 'btn btn-default' %>


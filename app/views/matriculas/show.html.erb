<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:aluno_id) %>:</strong></dt>
  <dd><%= @matricula.aluno_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:curso_id) %>:</strong></dt>
  <dd><%= @matricula.curso_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:data_da_matrÃ­cula) %>:</strong></dt>
  <dd><%= l(@matricula.data_matricula) %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ano) %>:</strong></dt>
  <dd><%= @matricula.ano %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ativo) %>:</strong></dt>
  <dd><%= @matricula.ativo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pago) %>:</strong></dt>
  <dd><%= @matricula.pago %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              matriculas_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_matricula_path(@matricula), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              matricula_path(@matricula),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
